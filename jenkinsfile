pipeline {
    agent any

    stages {
        stage('Checkout Code') {
            steps {
                git branch: 'master', url: 'https://github.com/mBuergi86/transfwork_cloud.git', credentialsId: 'todo_app_Jenkins'
            }
        }
        stage('Build NPM') { 
            steps {
                sh 'npm install' 
            }
        }
    }
}
